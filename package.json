{
  "name": "djia",
  "description": "The most recent Dow Jones opening for a date.",
  "version": "1.3.1",
  "author": "Luke Karrys",
  "browser": "lib/browser.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "bugs": {
    "url": "https://github.com/lukekarrys/djia/issues"
  },
  "dependencies": {
    "async": "^2.5.0",
    "debug": "^3.0.0",
    "flat-cache": "^1.2.2",
    "localforage": "^1.5.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "request": "^2.81.0",
    "xhr": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^8.0.0",
    "browserify": "^14.4.0",
    "electron": "^9.4.0",
    "git-validate": "^2.2.2",
    "nock": "^9.0.14",
    "rimraf": "^2.6.1",
    "run-browser": "^2.0.2",
    "standard": "^10.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-run": "^3.0.0",
    "xhr-mock": "^1.9.0"
  },
  "files": [
    "lib/*"
  ],
  "homepage": "https://github.com/lukekarrys/djia",
  "keywords": [
    "djia",
    "dow jones",
    "stock market",
    "xkcd"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "react-native": {
    "./lib/index.js": "./lib/react-native.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lukekarrys/djia.git"
  },
  "scripts": {
    "browser-test": "browserify test/browser.js | tape-run | tap-spec",
    "compile": "babel src --out-dir lib",
    "lint": "standard",
    "node-test": "babel-tape-runner test/{index,dowHasData}.js | tap-spec",
    "prepare": "npm run compile",
    "start": "run-browser test/browser.js",
    "test": "npm run node-test && npm run browser-test",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "**/lib/**"
    ]
  }
}
